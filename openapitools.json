{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.1.0",
    "generators": {
      "typescript-axios": {
        "generatorName": "typescript-axios",
        "output": "./frontend/src/api",
        "glob": "./openapi.yaml",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true,
          "withSeparateModelsAndApi": true,
          "apiPackage": "api",
          "modelPackage": "models"
        }
      }
    }
  }
}